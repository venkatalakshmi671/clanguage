#include<stdio.h>
void main()
{
    int a,b;
    printf("Enter two numbers");
    scanf("%d%d",&a,&b);
    int big=a>b?a:b;
    int step=big;
    while(1)
    {
        if(big%a==0 && big%b==0)
        {
            printf("lcm is %d",big);
            break;
        }
        else
        big=big+step;
    }
}
